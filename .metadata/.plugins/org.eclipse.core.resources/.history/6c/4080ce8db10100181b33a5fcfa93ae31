

/************************************
*          Personnel               *
*************************************/

function Personnel(nom, anEmbauche, salaireM){
	// Attributs
	this.nom=nom;
	this.anEmbauche=anEmbauche;
	this.salaireM=salaireM;
	// MÃ©thodes
	Personnel.getSalaireA = function(){
		return 12 * salaireM;
	}
	
	}